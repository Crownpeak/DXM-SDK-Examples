{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "crownpeak-dxm-angular-sdk-example-bootstrap-homepage": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": ".",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/crownpeak-dxm-angular-sdk-example-bootstrap-homepage",
            "index": "index.html",
            "main": "main.ts",
            "polyfills": "polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "scripts": [],
            "assets": ["./content"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "crownpeak-dxm-angular-sdk-example-bootstrap-homepage:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "crownpeak-dxm-angular-sdk-example-bootstrap-homepage:build:production"
            }
          }
        }
      }
    }},
  "defaultProject": "crownpeak-dxm-angular-sdk-example-bootstrap-homepage"
}
